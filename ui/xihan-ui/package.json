{
  "name": "xihan-ui",
  "version": "0.8.14",
  "description": "Fast, lightweight, efficient, and dedicated component library.Built on Vue.",
  "license": "MIT",
  "homepage": "https://ui.xihanfun.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/XiHanFun/XiHan.UI",
    "directory": "xihan-ui"
  },
  "bugs": {
    "url": "https://github.com/XiHanFun/XiHan.UI/issues"
  },
  "keywords": [
    "xihan",
    "ui"
  ],
  "author": {
    "name": "zhaifanhua",
    "email": "me@zhaifanhua.com",
    "url": "https://www.zhaifanhua.com"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "../dist/lib/xihan-ui/src/index.js",
  "module": "../dist/es/xihan-ui/src/index.mjs",
  "types": "../dist/es/xihan-ui/src/index.d.ts",
  "style": "../dist/es/xihan-ui/src/index.css",
  "exports": {
    ".": {
      "types": "../dist/es/xihan-ui/src/index.d.ts",
      "import": "../dist/es/xihan-ui/src/index.mjs",
      "require": "../dist/lib/xihan-ui/src/index.js",
      "style": "../dist/es/xihan-ui/src/index.css"
    },
    "./es": {
      "types": "../dist/es/xihan-ui/src/index.d.ts",
      "import": "../dist/es/xihan-ui/src/index.mjs"
    },
    "./es/*.mjs": {
      "types": "../dist/es/xihan-ui/src/*.d.ts",
      "import": "../dist/es/xihan-ui/src/*.mjs"
    },
    "./es/*": {
      "types": [
        "../dist/es/xihan-ui/src/*.d.ts",
        "../dist/es/xihan-ui/src/*/index.d.ts"
      ],
      "import": "../dist/es/xihan-ui/src/*.mjs"
    },
    "./lib": {
      "types": "../dist/lib/xihan-ui/src/index.d.ts",
      "require": "../dist/lib/xihan-ui/src/index.js"
    },
    "./lib/*.js": {
      "types": "../dist/lib/xihan-ui/src/*.d.ts",
      "require": "../dist/lib/xihan-ui/src/*.js"
    },
    "./lib/*": {
      "types": [
        "../dist/lib/xihan-ui/src/*.d.ts",
        "../dist/lib/xihan-ui/src/*/index.d.ts"
      ],
      "require": "../dist/lib/xihan-ui/src/*.js"
    },
    "./styles": {
      "import": "../dist/es/xihan-ui/src/index.css",
      "require": "../dist/lib/xihan-ui/src/index.css"
    },
    "./*": "./*"
  },
  "files": [
    "src",
    "../dist/es/xihan-ui",
    "../dist/lib/xihan-ui"
  ],
  "sideEffects": [
    "../dist/*",
    "themes/**/*.css",
    "../dist/es/components/*/styles/*",
    "../dist/lib/components/*/styles/*"
  ],
  "peerDependencies": {
    "vue": "*"
  },
  "dependencies": {
    "@xihan-ui/components": "workspace:*",
    "@xihan-ui/constants": "workspace:*",
    "@xihan-ui/directives": "workspace:*",
    "@xihan-ui/hooks": "workspace:*",
    "@xihan-ui/locales": "workspace:*",
    "@xihan-ui/themes": "workspace:*",
    "@xihan-ui/utils": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "*",
    "typescript": "*"
  }
}
